{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-empty-interface": false,
    "object-literal-sort-keys": true,
    "ordered-imports": true,
    "no-console": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "interface-name": true,
    "no-any": true
  },
  "defaultSeverity": "warning"
}
